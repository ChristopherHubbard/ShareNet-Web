{
  "name": "codius-smart-sharing",
  "version": "1.0.0",
  "description": "Smart device sharing using codius smart contracts",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node --inspect-brk ./distlib/src/server/index.js",
    "build": "npm run copy && tsc -p ./ && webpack --config ./distlib/webpack.config.js --mode development",
    "deploy": "aws s3 sync --acl public-read --sse --delete ./dist/ s3://www.dn.com",
    "copy": "copyfiles src/**/*.png distlib/ && copyfiles src/**/*.scss distlib/",
    "test": "jest --coverage --collectCoverageFrom=src/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristopherHubbard/codius-smart-sharing.git"
