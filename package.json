{
  "name": "codius-smart-sharing",
  "version": "1.0.0",
  "description": "Smart device sharing using codius smart contracts",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-dev && node --inspect-brk ./distlib/src/server/index.js",
    "build-dev": "npm run copy && tsc -p ./ && webpack --config ./distlib/webpack.config.js --mode development",
    "build-prod": "npm run copy && tsc -p ./ && webpack --config ./distlib/webpack.config.js --mode production",
    "deploy": "npm run build-prod && aws s3 sync dist/ s3://codius-smart-sharing-s3",
    "copy": "copyfiles src/**/assets/* distlib/ && copyfiles src/**/assets/ilp_icon.png dist/ copyfiles src/**/assets/paypal_icon.png dist/ copyfiles src/**/assets/manifest.json dist/",
    "test": "jest --coverage --collectCoverageFrom=src/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristopherHubbard/codius-smart-sharing.git"
  },
